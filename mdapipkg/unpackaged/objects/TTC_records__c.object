    <validationRules>
        <fullName>Link_TTC_to_a_Lead_or_Opportunity</fullName>
        <active>true</active>
        <description>Link_TTC_to_a_Lead_or_Opportunity</description>
        <errorConditionFormula>AND(ISBLANK(Lead__c),ISBLANK(Opportunity__c))</errorConditionFormula>
        <errorMessage>TTC collaboration needs to be linked to either Lead or Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Respond_by_date_should_be_in_future</fullName>
        <active>true</active>
        <description>Respond by date should be in future and minimum 5 days interval from today</description>
        <errorConditionFormula>Respond_by_Date__c &lt; TODAY()+5</errorConditionFormula>
        <errorDisplayField>Respond_by_Date__c</errorDisplayField>
        <errorMessage>Respond by date should be in future and minimum 5 days interval from today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TTC_text_areas_blank_validation_error</fullName>
        <active>true</active>
        <description>TTC_text_areas_blank_validation_error</description>
        <errorConditionFormula>OR(ISBLANK(Scope__c), ISBLANK(Technical_information_needed__c ), ISBLANK(Engagement_Summary__c), ISBLANK(TEXT(Environment__c) ))</errorConditionFormula>
        <errorMessage>Scope,Technical_information_needed, Engagement_Summary &amp; Environment__c cannot be blank</errorMessage>
    </validationRules>
